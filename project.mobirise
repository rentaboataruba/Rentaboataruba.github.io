                "overflow": "hidden",
                "box-sizing": "border-box"
              },
              ".marquee span": {
                "display": "inline-block",
                "padding-bottom": "(@menuTopLinePaddingBottom * 0.5px)",
                "padding-left": "unit(@TopLineEffectScrollTimeGap * 100, %)",
                "will-change": "transform",
                "-moz-animation": "marquee (@TopLineEffectScrollSpeed * 1s) linear infinite",
                "-webkit-animation": "marquee (@TopLineEffectScrollSpeed * 1s) linear infinite",
                "animation": "marquee (@TopLineEffectScrollSpeed * 1s) linear infinite"
              },
              "& when (@TopLineMouseHoverPause)": {
                ".marquee span:hover": {
                  "animation-play-state": "paused",
                  "-moz-animation-play-state": "paused",
                  "-webkit-animation-play-state": "paused"
                }
              },
              "@-moz-keyframes marquee": {
                "0%": {
                  "transform": "translate(0, 0)"
                },
                "100%": {
                  "transform": "translate(-100%, 0)"
                }
              },
              "@-webkit-keyframes marquee": {
                "0%": {
                  "transform": "translate(0, 0)"
                },
                "100%": {
                  "transform": "translate(-100%, 0)"
                }
              },
              "@keyframes marquee": {
                "0%": {
                  "transform": "translate(0, 0)"
                },
                "100%": {
                  "transform": "translate(-100%, 0)"
                }
              }
            },
            "img": {
