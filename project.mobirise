            ".mylogo": {
              "-webkit-transform": "rotate(@LogoRotate * 1deg)",
              "-moz-transform": "rotate(@LogoRotate * 1deg)",
              "-ms-transform": "rotate(@LogoRotate * 1deg)",
              "-o-transform": "rotate(@LogoRotate * 1deg)",
              "transform": "rotate(@LogoRotate * 1deg)",
