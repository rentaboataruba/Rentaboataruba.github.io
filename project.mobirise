                "& when (@menuTopLineGradientBgStyle = \"menuTopLineGradientBgTB2\")": {
                  "background": "linear-gradient(to bottom, @newMenuTopLineBgColor, @newMenuTopLineBgColor2) !important"
                },
                "& when (@menuTopLineGradientBgStyle = \"menuTopLineGradientBgTB3\")": {
                  "background": "linear-gradient(to bottom, @newMenuTopLineBgColor, @newMenuTopLineBgColor2, @newMenuTopLineBgColor3) !important"
                },
                "& when (@menuTopLineGradientBgStyle = \"menuTopLineGradientBgTB4\")": {
                  "background": "linear-gradient(to bottom, @newMenuTopLineBgColor, @newMenuTopLineBgColor2, @newMenuTopLineBgColor3, @newMenuTopLineBgColor4) !important"
                }
              },
              "margin": "0",
              "overflow": "hidden",
              "text-align": "center",
              "padding-top": "(@menuTopLinePaddingTop * 1px)",
              "padding-bottom": "(@menuTopLinePaddingBottom * 1px)",
              "color": "#ffffff"
            },
            ".topLineDivSettings span": {
              "font-weight": "@MenuTLFontWeight !important"
            },
            ".topLineTextSettings": {
              "margin": "0",
              "background": "none",
              "width": "100%",
              "padding-left": "5px",
              "padding-right": "5px",
              "margin-top": "0",
              "margin-right": "0",
              "margin-bottom": "0",
              "margin-left": "0",
              "line-height": "1"
            },
            "& when not (@collapsed)": {
              ".navbar-toggler:not(:disabled):not(.disabled)": {
                "cursor": "pointer",
                "margin-right": "10px"
              }
            },
            "& when (@TopLineEffect = 'TopLineEffectPulse')": {
              ".topline-msg": {
                "-moz-animation": "Pulse (@TopLineEffectPulseSpeed * 1s) ease-out",
                "-moz-animation-iteration-count": "@TopLineEffectPulseCount",
                "-webkit-animation": "Pulse (@TopLineEffectPulseSpeed * 1s) ease-out",
                "-webkit-animation-iteration-count": "@TopLineEffectPulseCount",
                "animation": "Pulse (@TopLineEffectPulseSpeed * 1s) ease-out",
                "animation-iteration-count": "@TopLineEffectPulseCount"
              },
              "@-moz-keyframes Pulse": {
                "0%": {
                  "opacity": "0.5"
                },
                "50%": {
                  "opacity": "1.0"
                },
                "100%": {
                  "opacity": "0.5"
                }
              },
              "@-webkit-keyframes Pulse": {
                "0%": {
                  "opacity": "0.5"
                },
                "50%": {
                  "opacity": "1.0"
                },
                "100%": {
                  "opacity": "0.5"
                }
              },
              "@keyframes Pulse": {
                "0%": {
                  "opacity": "0.5"
                },
                "50%": {
                  "opacity": "1.0"
                },
                "100%": {
                  "opacity": "0.5"
                }
              }
            },
            "& when (@TopLineEffect = 'TopLineEffectFlash')": {
              ".topline-msg": {
                "-moz-animation": "Flash (@TopLineEffectFlashSpeed * 1s) infinite",
                "-moz-animation-iteration-count": "@TopLineEffectFlashCount",
                "-webkit-animation": "Flash (@TopLineEffectFlashSpeed * 1s) infinite",
                "-webkit-animation-iteration-count": "@TopLineEffectFlashCount",
                "animation": "Flash (@TopLineEffectFlashSpeed * 1s) infinite",
                "animation-iteration-count": "@TopLineEffectFlashCount"
              },
              "@-moz-keyframes Flash": {
                "0%": {
                  "opacity": "1"
                },
                "50%": {
                  "opacity": "0"
                },
                "100%": {
                  "opacity": "1"
                }
              },
              "@-webkit-keyframes Flash": {
                "0%": {
                  "opacity": "1"
                },
                "50%": {
                  "opacity": "0"
                },
                "100%": {
                  "opacity": "1"
                }
              },
              "@keyframes Flash": {
                "0%": {
                  "opacity": "1"
                },
                "50%": {
                  "opacity": "0"
                },
                "100%": {
                  "opacity": "1"
                }
              }
            },
            "& when (@TopLineEffect = 'TopLineEffectScroll')": {
              ".marquee": {
                "width": "unit(@TopLineEffectScrollWindow, %)",
                "margin": "0 auto",
                "white-space": "nowrap",
                "overflow": "hidden",
                "box-sizing": "border-box"
              },
              ".marquee span": {
                "display": "inline-block",
                "padding-bottom": "(@menuTopLinePaddingBottom * 0.5px)",
                "padding-left": "unit(@TopLineEffectScrollTimeGap * 100, %)",
                "will-change": "transform",
                "-moz-animation": "marquee (@TopLineEffectScrollSpeed * 1s) linear infinite",
                "-webkit-animation": "marquee (@TopLineEffectScrollSpeed * 1s) linear infinite",
                "animation": "marquee (@TopLineEffectScrollSpeed * 1s) linear infinite"
              },
              "& when (@TopLineMouseHoverPause)": {
                ".marquee span:hover": {
                  "animation-play-state": "paused",
                  "-moz-animation-play-state": "paused",
                  "-webkit-animation-play-state": "paused"
                }
              },
              "@-moz-keyframes marquee": {
                "0%": {
                  "transform": "translate(0, 0)"
                },
                "100%": {
                  "transform": "translate(-100%, 0)"
                }
              },
              "@-webkit-keyframes marquee": {
                "0%": {
                  "transform": "translate(0, 0)"
                },
                "100%": {
                  "transform": "translate(-100%, 0)"
                }
              },
              "@keyframes marquee": {
                "0%": {
                  "transform": "translate(0, 0)"
                },
                "100%": {
                  "transform": "translate(-100%, 0)"
                }
              }
            },
            "img": {
              "width": "auto"
            },
            ".navbar-dropdown .navbar-brand span": {
              "vertical-align": "middle"
            },
            ".navbar-dropdown.navbar-short .navbar-logo": {
              "margin-right": "0.8rem"
            },
            ".navbar-dropdown .navbar-logo": {
              "margin-right": "0.8rem"
            },
            "a.btn": {
              "min-width": "auto !important",
              "line-height": "normal !important"
            },
            ".mylogo": {
              "-webkit-transform": "rotate(@LogoRotate * 1deg)",
              "-moz-transform": "rotate(@LogoRotate * 1deg)",
              "-ms-transform": "rotate(@LogoRotate * 1deg)",
              "-o-transform": "rotate(@LogoRotate * 1deg)",
              "transform": "rotate(@LogoRotate * 1deg)",
              "border-width": "(@LogoBorderWidth * 1px)",
              "border-color": "@LogoBorderColor",
              "border-radius": "(@LogoBorderRadius * 1px)",
              "margin-top": "(@LogoMarginTop * 1px)",
              "& when (@LogoBorderStyle = \"LogoBorderDotted\")": {
