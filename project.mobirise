                  "background": "linear-gradient(to right, @newMenuTopLineBgColor, @newMenuTopLineBgColor2, @newMenuTopLineBgColor3) !important"
                },
                "& when (@menuTopLineGradientBgStyle = \"menuTopLineGradientBgLR4\")": {
                  "background": "linear-gradient(to right, @newMenuTopLineBgColor, @newMenuTopLineBgColor2, @newMenuTopLineBgColor3, @newMenuTopLineBgColor4) !important"
                },
                "& when (@menuTopLineGradientBgStyle = \"menuTopLineGradientBgTB2\")": {
                  "background": "linear-gradient(to bottom, @newMenuTopLineBgColor, @newMenuTopLineBgColor2) !important"
                },
                "& when (@menuTopLineGradientBgStyle = \"menuTopLineGradientBgTB3\")": {
                  "background": "linear-gradient(to bottom, @newMenuTopLineBgColor, @newMenuTopLineBgColor2, @newMenuTopLineBgColor3) !important"
                },
                "& when (@menuTopLineGradientBgStyle = \"menuTopLineGradientBgTB4\")": {
                  "background": "linear-gradient(to bottom, @newMenuTopLineBgColor, @newMenuTopLineBgColor2, @newMenuTopLineBgColor3, @newMenuTopLineBgColor4) !important"
                }
              },
              "margin": "0",
              "overflow": "hidden",
              "text-align": "center",
              "padding-top": "(@menuTopLinePaddingTop * 1px)",
              "padding-bottom": "(@menuTopLinePaddingBottom * 1px)",
              "color": "#ffffff"
            },
            ".topLineDivSettings span": {
